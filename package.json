{
  "name": "fluentdb",
  "version": "0.0.43",
  "description": "Do dataset joins, mergings, filters, mappings, orderings, groupings, foldings (aggregations), visualizations and similar operations on plain old javascript memory, indexedDb, mongoDb, or hybrid structures.  A 'Dataset' is an array of objects with named properties.  This code is NOT ready for production.  Inspired by c# linq method syntax.",
  "main": ".dist/FluentDB.server.next.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "gitPublish": "node_scripts\\gitPublish.cmd",
    "gitPublishWiki": "node_scripts\\gitPublishWiki.cmd",
    "gitCommitWiki": "node_scripts\\gitCommitWiki.cmd",
    "home2readme": "node node_scripts\\home2readme.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/paulwilcox/FluentDB"
  },
  "keywords": [
    "linq",
    "join",
    "merge",
    "fold",
    "aggregate",
    "order",
    "group",
    "dataset",
    "print",
    "display",
    "visualize",
    "filter",
    "map",
    "IndexedDb",
    "MongoDB"
  ],
  "author": "Paul Wilcox",
  "license": "ISC",
  "dependencies": {},
  "optionalDependencies": {
    "mongodb": "^3.2.5"
  },
  "devDependencies": {
    "console.table": "^0.10.0",
    "glimp": "0.0.3",
    "puppeteer": "^1.19.0",
    "rollup": "^1.32.0",
    "rollup-plugin-license": "^0.13.0",
    "sampledb": "0.0.9",
    "sctester": "0.0.21"
  }
}
