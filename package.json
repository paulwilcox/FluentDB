{
  "name": "fluentdb",
  "version": "0.0.48",
  "description": "Do dataset joins, mergings, filters, mappings, orderings, groupings, mergings, reducings (aggregations), and similar operations on datast style objects.  A 'Dataset' is an array of objects with named properties.  Inspired by c# linq method syntax.",
  "main": ".dist/FluentDB.server.next.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "gitPublish": "node_scripts\\gitPublish.cmd",
    "gitPublishWiki": "node_scripts\\gitPublishWiki.cmd",
    "gitCommitWiki": "node_scripts\\gitCommitWiki.cmd",
    "home2readme": "node node_scripts\\home2readme.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/paulwilcox/FluentDB"
  },
  "keywords": [
    "dataset",
    "array",
    "iterable",
    "map",
    "filter",
    "merge",
    "join",
    "fold",
    "reduce",
    "aggregate",
    "order",
    "sort",
    "group",
    "ungroup",
    "linq"
  ],
  "author": "Paul Wilcox",
  "license": "ISC",
  "dependencies": {},
  "optionalDependencies": {
    "mongodb": "^3.2.5"
  },
  "devDependencies": {
    "console.table": "^0.10.0",
    "glimp": "0.0.3",
    "puppeteer": "^1.19.0",
    "rollup": "^1.32.0",
    "rollup-plugin-license": "^0.13.0",
    "sampledb": "0.0.9"
  }
}
