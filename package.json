    "url": "https://github.com/paulwilcox/oneQuery"
  },
  "keywords": [
    "linq",
    "join",
    "merge",
    "fold",
    "aggregate",
    "order",
    "group",
    "dataset",
    "print",
    "display",
    "visualize",
    "filter",
    "map",
    "IndexedDb",
    "MongoDB"
  ],
